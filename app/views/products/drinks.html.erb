<div class="wrapper">
  <div class="placeholder-top"></div>
  <div class="product-title d-flex justify-content-between p-2">
    <%= link_to :back, class: "previous-button" do %>
      <div class="img-btn-previous"><%= image_tag "button-previous.png" %></div>
    <% end %>
    <%# <input type="button" onclick="history.back()"> %>
    <h1>Drinks</h1>
    <%= link_to restaurant_restaurant_meals_path(@restaurant, {table: @table}), class: "previous-button" do %>
      <div class="img-btn-next"><%= image_tag "Button-next.png" %></div>
    <% end %>
  </div>
  <div class="accordion p-2 d-grid gap-3" id="accordionPanelsStayOpenExample" data-controller="cartcounter">
    <%= render partial: "categories_accordion", locals: { categories: @categories, table: @table, anchor: "Tapas"} %>
    <%= render partial: "cart_button" %>
  </div>
</div>
